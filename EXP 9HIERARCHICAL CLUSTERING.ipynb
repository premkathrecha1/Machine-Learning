    "import matplotlib.pyplot as plt\n",
    "from sklearn.datasets import load_iris\n",
    "from scipy.cluster.hierarchy import dendrogram, linkage\n",
    "\n",
    "data = load_iris()\n",
    "X = data.data\n",
    "\n",
    "linked = linkage(X, method='ward')\n",
    "plt.figure(figsize=(10,6))\n",
    "dendrogram(linked)\n",
    "plt.show()\n"
   ]
  }
