   "source": [
    "from sklearn.cluster import DBSCAN\n",
    "from sklearn.datasets import load_iris\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "data = load_iris()\n",
    "X = data.data\n",
    "\n",
    "db = DBSCAN(eps=0.5, min_samples=5)\n",
    "labels = db.fit_predict(X)\n",
    "\n",
    "plt.scatter(X[:,0], X[:,1], c=labels)\n",
    "plt.show()\n"
   ]
  }
